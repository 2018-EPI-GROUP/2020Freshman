char *match( char *s, char ch1, char ch2 )
{   
    int i,c=0,k=0,j;
    for(i=0;*(s+i)!=0;i++)
    {
        if(*(s+i)==ch1)
    {
        k=i;
            break;
    }
    }
    for(j=k;*(s+j)!=0;j++)
    {
        if(*(s+j)==ch2)
        {
            c=j;
            break;
        }
    }
    if(k==0)
    {  return 1;
        
    }
    if(j==0)
       return (s+k);
    if(k!=0&&j!=0)
        return 0;
}
